<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorPane" prefHeight="290.0" prefWidth="325.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.C_Login">
   <children>
      <Button fx:id="buttonLogin" layoutX="52.0" layoutY="121.0" mnemonicParsing="false" onAction="#login" text="Anmelden" />
      <Button fx:id="buttonRegister" layoutX="200.0" layoutY="121.0" mnemonicParsing="false" onAction="#switchToRegister" text="Registrieren" />
      <Button fx:id="buttonClose" layoutX="217.0" layoutY="253.0" mnemonicParsing="false" onAction="#closeApp" text="Beenden" />
      <TextField fx:id="textFieldEmail" layoutX="49.0" layoutY="49.0" prefHeight="25.0" prefWidth="231.0" promptText="Email Adresse" />
      <PasswordField fx:id="passwordField" layoutX="49.0" layoutY="83.0" prefHeight="25.0" prefWidth="231.0" promptText="Password" />
      <Label fx:id="labelInfo" layoutX="49.0" layoutY="29.0" prefHeight="17.0" prefWidth="231.0" text="Label" textFill="#c60d0d">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
